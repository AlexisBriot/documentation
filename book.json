{
  "plugins": [
    "edit-link",
    "prism", 
    "-highlight", 
    "github",
    "include-codeblock"
  ],
  "pluginsConfig": {
    "edit-link": {
      "base": "https://github.com/mesg-tech/documentation/tree/master",
      "label": "Edit This Page"
    },
    "github": {
      "url": "https://github.com/mesg-tech/documentation/"
    },
    "include-codeblock": {
      "template": "default"
    }
  },
  "links": {
    "sharing": {
      "facebook": false,
      "twitter": false
    }
  },
  "variables": {
    "token": "MESG token",
    "network": "MESG decentralised network",
    "wallet": "[MESG Wallet](/installation.md)",
    "cli": "MESG CLI",
    "endpoints": {
      "services": "https://mesg.tech/services",
      "validationSchema": "https://github.com/mesg-tech/validation-schema"
    }
  }
}